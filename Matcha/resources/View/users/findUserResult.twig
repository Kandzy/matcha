{% extends "users/findUser.twig" %}

{% block result %}
    </br>
    {% for user in users %}
        <img style="height: 16vw; width: 14vw;" src="{{ user.Avatar }}">
        <p style="color:white; display: flex; justify-content: center">{{ user.Login }}
        <a href="{{ path_for('profile', {username :user.Login}) }}"><button>Go profile</button></a></p>
        </br>
    {% endfor %}
{% endblock %}